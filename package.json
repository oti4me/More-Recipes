{
  "name": "more-recipe",
  "version": "1.0.0",
  "private": true,
  "nyc": {
    "exclude": ""
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --config ./webpack.config.prod.babel.js",
    "start:dev": "NODE_ENV=development nodemon  ./server/server --watch ./server --exec babel-node",
    "test": "npm run migrate:test",
    "test:dev": "npm run migrate:test",
    "jest": "jest --coverage --collectCoverageFrom=client/**/**/*.{js,jsx}",
    "webpack": "webpack -p",
    "test-dev": "cross-env NODE_ENV=test mocha ./server/test/* --compilers js:babel-register",
    "test:nyc": "NODE_ENV=test nyc --reporter=html --reporter=text mocha --timeout 500000 --compilers js:babel-core/register server/test/*",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "migration:test": "NODE_ENV=test sequelize db:migrate",
    "migration": "NODE_ENV=production sequelize db:migrate",
    "migration:dev": "sequelize db:migrate",
    "migrate:test": "NODE_ENV=test npm run migration:undo:test && npm run migration:test && npm run test:nyc",
    "migration:undo:test": "NODE_ENV=test sequelize db:migrate:undo:all",
    "start": "node ./dist/server/server.js",
    "prod:server": "babel -d ./dist/server ./server -s",
    "prod:client": "webpack --config webpack.prod.js",
    "build:prod": "npm run prod:client && npm run prod:server",
    "heroku-postbuild": "npm run prod:client && npm run prod:server",
    "heroku-prebuild": "npm i -g babel-cli",
    "build:run": "npm run build:prod && npm start"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.16.0",
    "cloudinary": "^1.9.1",
    "cookie-parser": "~1.4.3",
    "coveralls": "^3.0.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.9",
    "debug": "~2.6.0",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.5.4",
    "eslint": "^4.7.2",
    "express": "~4.14.1",
    "express-favicon": "^2.0.0",
    "express-validator": "^4.2.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-exclude-assets-plugin": "0.0.5",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "jquery": "^3.2.1",
    "jsonwebtoken": "^8.0.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "mocha": "^3.5.3",
    "moment": "^2.20.1",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "node-sass": "^4.7.2",
    "nyc": "^11.3.0",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-md-spinner": "^0.2.5",
    "react-paginate": "^5.0.0",
    "react-paginate-component": "0.0.3",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-persist": "^5.0.0",
    "redux-persist-store": "^0.2.4",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "sequelize": "^4.13.2",
    "sequelize-cli": "^3.0.0",
    "serve-favicon": "~2.3.2",
    "shortid": "^2.2.8",
    "style-loader": "^0.19.1",
    "supertest": "^3.0.0",
    "validate-password": "^1.0.4",
    "validator": "^9.0.0",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-hot-middleware": "^2.21.0"
  },
  "devDependencies": {
    "babel-jest": "^22.1.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "faker": "^4.1.0",
    "favicons-webpack-plugin": "0.0.7",
    "jest": "^22.1.4",
    "jsdoc": "^3.5.5",
    "nodemon": "^1.12.1",
    "react-test-renderer": "^16.2.0"
  }
}